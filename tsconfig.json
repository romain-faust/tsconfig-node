{
	"compilerOptions": {
		"alwaysStrict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"importsNotUsedAsValues": "error",
		"lib": ["ES2021"],
		"module": "CommonJS",
		"moduleResolution": "Node",
		"newLine": "LF",
		"noEmitOnError": true,
		"noErrorTruncation": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"pretty": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ES2021",
		"types": ["node"]
	}
}
